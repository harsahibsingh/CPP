//Computing Power
//Ex: Input: 2 3
// Output: 8
//Naive Solution : Time Complexity: O(n)

#include<iostream>

using namespace std;


int main()
{
    int x=0;
    int y=0;
    int res=1;
    
    cout<<"Enter numbers to Compute Powers : ";
    cin>>x>>y;
    
    for(int i=0;i<y;i++)
    {
        res= res*x;
    }
    
    cout<<"Output: "<<res<<endl; 
    

    return 0;
}
