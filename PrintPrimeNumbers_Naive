//Enter a num and find all prime numbers less than it
//Ex: Input: 10
// Output: 2 3 5 7
//Naive Solution
//Time Complexity: O(n * âˆšn)

#include<iostream>

using namespace std;

bool isPrime(int num)
{
    if(num ==2 || num ==3)
    {
        return true;
    }
    
    if(num%2 ==0 || num%3 ==0)
    {
        return false;
    }
    
    for(int i=5;i*i<num;i++)
    {
        if(num%i == 0)
        {
            return false;
        }
    }
    
    return true;
}

void PrintPrimeNumbers(int num)
{
    for(int i=2;i<num;i++)
    {
        if(isPrime(i))
        {
            cout<<i<<" ";
        }
    }
}


int main()
{
    int num=0;
    
    cout<<"Enter number to print Prime Numbers: ";
    cin>>num;
    
    PrintPrimeNumbers(num);
    

    return 0;
}
